<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MobileApp.Views.PartyPlayerDetailPage">
    <ContentPage.Content>
        <StackLayout>
            <Label x:Name="Name" Text="{Binding UserName}" />
            <Label x:Name="Race" Text="{Binding Race}"/>
            <Label x:Name="Class" Text="{Binding Class}"></Label>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>